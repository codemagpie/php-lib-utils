{
    "name": "codemagpie/utils",
    "type": "library",
    "description": "A utils",
    "keywords": [
        "codemagpie",
        "utils"
    ],
    "authors": [
        {
            "name": "lyf",
            "email": "1191628562@qq.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.1",
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1"
    },
    "archive": {
        "exclude": ["/tests", ".gitattributes", ".gitignore", ".php-cs-fixer.php"]
    },
    "prefer-stable": true,
    "config": {
        "secure-http": false
    },
    "autoload": {
        "psr-4": {
            "CodeMagpie\\Utils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeMagpie\\UtilsTests\\": "tests/"
        }
    }
}
